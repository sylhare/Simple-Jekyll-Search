{
  "name": "simple-jekyll-search",
  "version": "1.10.1",
  "description": "Fork of Simple Jekyll Search from https://github.com/christian-fei/Simple-Jekyll-Search",
  "main": "dest/simple-jekyll-search.js",
  "type": "module",
  "scripts": {
    "cypress": "cypress",
    "lint": "eslint . --ext .ts",
    "pretest": "npm run lint",
    "prebuild": "npm run test",
    "postbuild": "node scripts/stamp.js < dest/simple-jekyll-search.min.js > dest/simple-jekyll-search.min.js.tmp && mv dest/simple-jekyll-search.min.js.tmp dest/simple-jekyll-search.min.js && npm run copy-example-code",
    "build": "vite build && terser dest/simple-jekyll-search.js -o dest/simple-jekyll-search.min.js",
    "dist": "npm run build && npm run copy-example-code",
    "test": "vitest run",
    "test:watch": "vitest",
    "start": "cd example; jekyll serve",
    "copy-example-code": "cp dest/* example/js/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sylhare/Simple-Jekyll-Search.git"
  },
  "author": "Sylhare (Original: Christian Fei)",
  "license": "MIT",
  "files": [
    "dest",
    "src"
  ],
  "bugs": {
    "url": "https://github.com/christian-fei/Simple-Jekyll-Search/issues"
  },
  "homepage": "https://github.com/christian-fei/Simple-Jekyll-Search",
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "cypress": "^14.1.0",
    "eslint": "^8.57.0",
    "terser": "^5.39.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^6.2.4",
    "vitest": "^3.1.1"
  }
}
